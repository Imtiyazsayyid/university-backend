{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@": "./src",
          "@core": "./src/@core",
          "underscore": "lodash"
        }
      }
    ],
    ["@babel/transform-runtime"]
  ]
}
